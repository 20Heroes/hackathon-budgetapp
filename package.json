{
  "name": "budget-hackathon-pallet",
  "version": "1.0.0",
  "description":
    "App for budget controller original from a Udemy course and used for hackathons",
  "main": "app.js",
  "scripts": {
    "test": "mocha src/**/**.test.js",
    "test-watch": "nodemon --exec \"npm test\"",
    "precommit": "lint-staged",
    "watch": "parcel src/index.html --out-dir public/",
    "build": "parcel build src/index.html --out-dir dist/"
  },
  "lint-staged": {
    "src/**/*.js": ["eslint", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/20Heroes/hackathon-budgetapp.git"
  },
  "keywords": ["Budget", "hackathon", "javascript", "node"],
  "author": "Guilherme Scaldelai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/20Heroes/hackathon-budgetapp/issues"
  },
  "homepage": "https://github.com/20Heroes/hackathon-budgetapp#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.5",
    "parcel-bundler": "^1.6.2",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0"
  }
}
