{
  "name": "budget-hackathon-pallet",
  "version": "1.0.0",
  "description": "App for budget controller original from a Udemy course and used for hackathons",
  "main": "server/app.js",
  "scripts": {
    "watch": "nodemon --ignore public/",
    "start": "node server/app.js",
    "dev:start": "concurrently \"npm run watch\" \"npm run parcel:watch\" --names \"NODEMON ðŸ’»,PARCEL ðŸ“¦ \" --prefix name",
    "dev:up": "docker-compose up",
    "dev:down": "docker-compose down",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "lint-staged",
    "parcel:watch": "parcel src/index.html --out-dir public/ --hmr-port 80",
    "parcel:build": "parcel build src/index.html --out-dir dist/"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/20Heroes/hackathon-budgetapp.git"
  },
  "keywords": [
    "Budget",
    "hackathon",
    "javascript",
    "node"
  ],
  "author": "Guilherme Scaldelai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/20Heroes/hackathon-budgetapp/issues"
  },
  "homepage": "https://github.com/20Heroes/hackathon-budgetapp#readme",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.17.2",
    "parcel-bundler": "^1.6.2",
    "prettier": "^1.11.1"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
}
